{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "Model/*": [
        "./src/models/*"
      ],
      "Route/*": [
        "./src/routes/*"
      ],
      "Util": [
        "./src/utils/index"
      ],
      "Util/*": [
        "./src/utils/*"
      ],
      "Constant/*": [
        "./src/constants/*"
      ],
      "Constant": [
        "./src/constants"
      ],
      "Controller": ["./scr/controllers/index"],
      "Controller/*": ["./scr/controllers/*"]
    }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
  "include": [
    "src/**/*"
, "config", "seeders", "migrations"  ]
}